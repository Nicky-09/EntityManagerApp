{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "task": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "title": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "extra": {
            "type": "object",
            "properties": {
              "start_date": {
                "type": "string",
                "format": "date"
              },
              "end_date": {
                "type": "string",
                "format": "date"
              }
            },
            "required": ["start_date", "end_date"]
          }
        },
        "required": ["id", "title", "description", "extra"]
      }
    },
    "cars": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "car_id": {
            "type": "integer"
          },
          "car_name": {
            "type": "string"
          },
          "car_details": {
            "type": "object",
            "properties": {
              "car_number": {
                "type": "integer"
              },
              "car_engine": {
                "type": "string"
              },
              "car_model": {
                "type": "string"
              }
            },
            "required": ["car_number", "car_engine", "car_model"]
          }
        },
        "required": ["car_id", "car_name", "car_details"]
      }
    },
    "employee": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "employee_id": {
            "type": "string"
          },
          "position": {
            "type": "string"
          },
          "employee_details": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "address": {
                "type": "string"
              }
            },
            "required": ["name", "address"]
          }
        },
        "required": ["employee_id", "position", "employee_details"]
      }
    }
  },
  "required": ["task", "cars", "employee"]
}
